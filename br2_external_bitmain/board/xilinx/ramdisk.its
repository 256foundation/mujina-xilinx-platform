/dts-v1/;

/ {
    description = "Ramdisk";
    #address-cells = <1>;

    images {
        ramdisk {
            description = "Ramdisk";
            data = /incbin/("rootfs.ext2.gz");
            type = "ramdisk";
            arch = "arm";
            os = "linux";
            compression = "gzip";
        };
    };

    configurations {
        default = "config-1";

        config-1 {
            description = "Default configuration";
            ramdisk = "ramdisk";
        };
    };
};
